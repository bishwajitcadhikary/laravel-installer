{
    "name": "bishwajitcadhikary/laravel-installer",
    "description": "This is my package installer",
    "keywords": [
        "bishwajitcadhikary",
        "laravel",
        "installer"
    ],
    "homepage": "https://github.com/bishwajitcadhikary/installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Bishwajit Adhikary",
            "email": "bishwajitcadhikary@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "brotzka/laravel-dotenv-editor": "^2.2",
        "illuminate/contracts": "^10.0",
        "php-flasher/flasher-laravel": "^1.13",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AbnDevs\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbnDevs\\Installer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbnDevs\\Installer\\InstallerServiceProvider",
                "Brotzka\\DotenvEditor\\DotenvEditorServiceProvider"
            ],
            "aliases": {
                "Installer": "AbnDevs\\Installer\\Facades\\Installer",
                "License": "AbnDevs\\Installer\\Facades\\License",
                "DotenvEditor": "Brotzka\\DotenvEditor\\DotenvEditorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
